<h1>Tasks</h1>

<%@tasks.each do |task|  %>
  <ul id="tasks">
    <li>  <%= link_to "#{task.title}", task_path(task) %></li>
    <li><%= task.completed %></li>
    <li>  <%= link_to "Delete", delete_path(task), method: :delete, data: {confirm: "Are you sure?"} %></li>
  </ul>
<% end %>

<p> <%= link_to "Add a new Task", tasks_new_path %> </p>
